<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EditTextWithImage">
        <attr name="placeholder" format="string"/>
        <attr name="confidential" format="boolean"/>
        <attr name="image" format="reference|integer"/>
        <attr name="imagePosition" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
        <attr name="textSize" format="dimension"/>
        <attr name="text" format="string"/>
    </declare-styleable>
    <declare-styleable name="TextViewWithButton">
        <attr name="sectionHeaderText" format="string"/>
        <attr name="sectionHeaderTextSize" format="dimension"/>
        <attr name="setSectionHeaderVisible" format="boolean"/>
        <attr name="setBorder" format="boolean"/>
        <attr name="sectionTextSize" format="dimension"/>
    </declare-styleable>
</resources>